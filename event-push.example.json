{
  "targets": [
    { "url": "http://localhost:34567" },
    {
      "url": "{env:WEBHOOK_URL}",
      "events": [
        "session.created",
        "session.idle",
        "session.error"
      ],
      "retry": {
        "attempts": 3,
        "delayMs": 500
      },
      "headers": {
        "Authorization": "Bearer {env:WEBHOOK_TOKEN}"
      }
    },
    {
      "url": "https://logging.example.com/ingest",
      "events": [
        "tool.execute.after",
        "file.edited"
      ],
      "headers": {
        "X-API-Key": "{env:LOGGING_API_KEY}"
      }
    }
  ]
}
